                        <div id="answer-218390" class="answer" data-answerid="218390"   itemscope itemtype="http://schema.org/Answer">
                            <div class="post-layout">
                                <div class="votecell post-layout--left">


                                    <div class="vote">
                                        <input type="hidden" name="_id_" value="218390">
                                        <a class="vote-up-off" title="This answer is useful (click again to undo)">up vote</a>
                                        <span itemprop="upvoteCount" class="vote-count-post ">769</span>
