                        <div id="answer-218510" class="answer accepted-answer" data-answerid="218510"   itemprop="acceptedAnswer" itemscope itemtype="http://schema.org/Answer">
                            <div class="post-layout">
                                <div class="votecell post-layout--left">


                                    <div class="vote">
                                        <input type="hidden" name="_id_" value="218510">
                                        <a class="vote-up-off" title="This answer is useful (click again to undo)">up vote</a>
                                        <span itemprop="upvoteCount" class="vote-count-post high-scored-post">3252</span>
                                        <a class="vote-down-off" title="This answer is not useful (click again to undo)">down vote</a>



                                        <span class="vote-accepted-on load-accepted-answer-date" title="loading when this answer was accepted...">accepted</span>


